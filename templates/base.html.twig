<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>

        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <link rel="stylesheet" href="https://foundswatch.com/themes/sandstone/foundation.min.css" />
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
                    <link rel="stylesheet" href="{{ asset('style.css')}}">

        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <header class="top-bar top-bar-left">
    <ul class="dropdown menu">
            <li><a href="{{ path('loan_index') }}">Emprunt</a></li>
             <li><a href="{{ path('group_index') }}">Groupe</a></li>
             <li><a href="{{ path('category_index') }}">Category</a></li>
            <li><a href="{{ path('user_index') }}">User</a></li>
            <li><a href="{{ path('ressource_index') }}">Ressources</a></li>

        </ul>
    </header>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
